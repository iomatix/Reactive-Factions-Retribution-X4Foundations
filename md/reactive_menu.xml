					]" />
			</actions>
		</cue>
		<cue name="OnSMOSCheckboxChange" instantiate="true">
			<conditions>
				<event_cue_signalled />
			</conditions>
			<actions>
				<!-- <debug_text text="event.param.$id + ' to: ' + @event.param.$value + ' or ' + @event.param.$checked" /> -->
				<debug_text text="event.param.$id + ' pre: ' + md.ReactiveMain.ReactiveInit.{'$' + event.param.$id}" />
				<set_value name="md.ReactiveMain.ReactiveInit.{'$' + event.param.$id}" exact="event.param.$checked" />
				<debug_text text="event.param.$id + ' post: ' + md.ReactiveMain.ReactiveInit.{'$' + event.param.$id}" />
			</actions>
		</cue>
		<cue name="OnSMOSliderChange" instantiate="true">
			<conditions>
				<event_cue_signalled />
			</conditions>
			<actions>
				<!-- <debug_text text="event.param.$id + ' to: ' + @event.param.$value + ' or ' + @event.param.$checked" /> -->
				<debug_text text="event.param.$id + ' pre: ' + md.ReactiveMain.ReactiveInit.{'$' + event.param.$id}" />
				<set_value name="md.ReactiveMain.ReactiveInit.{'$' + event.param.$id}" exact="event.param.$value" />
				<debug_text text="event.param.$id + ' post: ' + md.ReactiveMain.ReactiveInit.{'$' + event.param.$id}" />
			</actions>
		</cue>
		<!-- special fix for CR values cuz Mod APIs is handling Cr values different than vanilla. -->
		<cue name="OnSMOSliderChangeCRvalue" instantiate="true">
			<conditions>
				<event_cue_signalled />
			</conditions>
			<actions>
				<!-- <debug_text text="event.param.$id + ' to: ' + @event.param.$value + ' or ' + @event.param.$checked" /> -->
				<debug_text text="event.param.$id + ' pre: ' + md.ReactiveMain.ReactiveInit.{'$' + event.param.$id}" />
				<set_value name="md.ReactiveMain.ReactiveInit.{'$' + event.param.$id}" exact="event.param.$value*1Cr" />
				<!-- the fix is *1Cr (it should append 00 to the value) -->
				<debug_text text="event.param.$id + ' post: ' + md.ReactiveMain.ReactiveInit.{'$' + event.param.$id}" />
			</actions>
		</cue>
	</cues>
</mdscript>